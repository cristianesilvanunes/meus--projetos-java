    //complete o c√≥digo abaixo para caucular o fatorial
    int  fatorial = 1;
    for (int i = 1; i <= 10; i++){
    
    System.out.print(" x " + i + " = " + ( num * i));  
    
    }
    scanner.close(); 
  }
}

        
